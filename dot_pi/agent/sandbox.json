{
  "enabled": true,
  "uv": {
    "enabled": true
  },
  "network": {
    "allowedDomains": ["*"],
    "deniedDomains": []
  },
  "filesystem": {
    "denyRead": [
      "~/.ssh",
      "~/.aws",
      "~/.gnupg",
      "~/.npmrc",
      "~/.netrc",
      "~/.git-credentials",
      "~/.kube/**",
      "~/.config/gcloud/**",
      "~/.azure/**",
      "~/.terraform.d/credentials.tfrc.json",
      "~/.pypirc",
      "~/.config/pip/pip.conf",
      "~/.config/gh/hosts.yml",
      "~/.gem/credentials",
      "**/.env*",
      "**/*.pem",
      "**/*.key",
      "**/*.p12",
      "**/*.pfx",
      "**/.npmrc",
      "**/.netrc",
      "**/.git-credentials",
      "**/.pypirc",
      "**/pip.conf",
      "**/.yarnrc.yml",
      "**/secrets/**",
      "**/credentials/**",
      "**/.aws/**",
      "**/.ssh/**",
      "**/.kube/**",
      "**/.config/gcloud/**",
      "**/.azure/**",
      "**/.terraform.d/credentials.tfrc.json",
      "**/.config/gh/hosts.yml",
      "**/.gem/credentials",
      "**/id_rsa",
      "**/id_ed25519"
    ],
    "allowWrite": [
      ".",
      "/tmp",
      "~/.pi/**",
      "~/Code/**",
      "~/.config/chezmoi/**",
      "~/.npm/**",
      "~/.bun/install/cache/**",
      "~/Library/Caches/**",
      "~/Library/pnpm/store/**",
      "~/.yarn/**",
      "~/.cache/yarn/**"
    ],
    "denyWrite": [
      "~/.ssh",
      "~/.aws",
      "~/.gnupg",
      "~/.npmrc",
      "~/.netrc",
      "~/.git-credentials",
      "~/.kube/**",
      "~/.config/gcloud/**",
      "~/.azure/**",
      "~/.terraform.d/credentials.tfrc.json",
      "~/.pypirc",
      "~/.config/pip/pip.conf",
      "~/.config/gh/hosts.yml",
      "~/.gem/credentials",
      "**/.env*",
      "**/*.pem",
      "**/*.key",
      "**/*.p12",
      "**/*.pfx",
      "**/.npmrc",
      "**/.netrc",
      "**/.git-credentials",
      "**/.pypirc",
      "**/pip.conf",
      "**/.yarnrc.yml",
      "**/secrets/**",
      "**/credentials/**",
      "**/.aws/**",
      "**/.ssh/**",
      "**/.kube/**",
      "**/.config/gcloud/**",
      "**/.azure/**",
      "**/.terraform.d/credentials.tfrc.json",
      "**/.config/gh/hosts.yml",
      "**/.gem/credentials",
      "**/id_rsa",
      "**/id_ed25519"
    ]
  }
}
