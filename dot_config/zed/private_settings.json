    "auto_fold_dirs": false,
    "button": true,
    "auto_reveal_entries": false,
    "entry_spacing": "standard",
  },
  "outline_panel": {
    "dock": "right",
    "button": true,
  },
  "collaboration_panel": {
    "default_width": 300,
    "button": true,
  },
  "notification_panel": {
    "button": true,
  },
  "git_panel": {
    "sort_by_path": true,
    "default_width": 300,
    "button": true,
  },
  "message_editor": {
    "auto_replace_emoji_shortcode": false,
  },
  "tabs": {
    "show_diagnostics": "errors",
    "git_status": true,
    "file_icons": true,
    "close_position": "right",
    "activate_on_close": "neighbour",
  },
  "file_finder": {
    "modal_max_width": "medium",
    "include_ignored": "all",
  },
  "active_pane_modifiers": {
    "inactive_opacity": 0.7,
    "border_size": 0.5,
  },
  "scrollbar": {
    "show": "system",
  },
  "preferred_line_length": 140,
  "show_wrap_guides": true,
  "auto_signature_help": false,
  "ui_font_size": 13.0,
  "buffer_font_size": 13.0,
  "vim_mode": true,
  "theme": "Tokyo Night",
  "icon_theme": "Material Icon Theme",
  "buffer_font_family": "FiraCode Nerd Font",
  "buffer_font_features": {
    "liga": true,
    "ss02": true,
    "ss03": true,
    "ss05": true,
    "ss07": true,
  },
  "cursor_blink": false,
  // prevent the buffer from scrolling beyond the last line
  "scroll_beyond_last_line": "off",
  // allow cursor to reach edges of screen
  "vertical_scroll_margin": 0,
  "inlay_hints": {
    "show_value_hints": true,
    "enabled": true,
  },
  "vim": {
    "toggle_relative_line_numbers": false,
    "use_system_clipboard": "never",
    "use_smartcase_find": true,
    "highlight_on_yank_duration": 500,
  },
  "use_smartcase_search": true,
  "toolbar": {
    "code_actions": false,
    "breadcrumbs": true,
    "quick_actions": true,
    "selections_menu": true,
  },
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware",
  },
  "theme_overrides": {},
  "debugger": {
    "button": true,
  },
  "terminal": {
    "toolbar": { "breadcrumbs": false },
    "line_height": "standard",
    "scrollbar": {
      "show": "never",
    },
    "button": true,
    "max_scroll_history_lines": 50000,
    "blinking": "off",
    "font_family": "FiraCode Nerd Font",
    "font_features": {
      "liga": true,
      "ss02": true,
      "ss03": true,
      "ss05": true,
      "ss07": true,
    },
    "detect_venv": {
      "on": {
        "directories": [".env", "env", ".venv", "venv"],
        "activate_script": "fish",
      },
    },
    "env": {
      "GIT_EDITOR": "zed --wait",
      "EDITOR": "zed",
    },
    "minimum_contrast": 0,
    "keep_selection_on_copy": false,
    "working_directory": "current_project_directory",
  },
  "load_direnv": "shell_hook",
  "file_types": {
    "Ansible": [
      "**.ansible.yml",
      "**/defaults/**.yml",
      "**/defaults/**.yaml",
      "**/meta/**.yml",
      "**/meta/**.yaml",
      "**/tasks/**.yml",
      "**/tasks/*.yml",
      "**/tasks/*.yaml",
      "**/handlers/*.yml",
      "**/handlers/*.yaml",
      "**/group_vars/**.yml",
      "**/group_vars/**.yaml",
      "**playbook*.yaml",
      "**playbook*.yml",
    ],
    "Dockerfile": ["Dockerfile*"],
    "Helm": [
      "**/templates/**/*.tpl",
      "**/templates/**/*.yaml",
      "**/templates/**/*.yml",
      "**/helmfile.d/**/*.yaml",
      "**/helmfile.d/**/*.yml",
      "**/values*.yaml",
    ],
    "Shell Script": ["*.envrc*"],
    "Nginx": ["**/nginx/**/*.conf", "nginx.conf"],
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy",
        },
        "cargo": {
          "allFeatures": true,
          "loadOutDirsFromCheck": true,
          "buildScripts": {
            "enable": true,
          },
        },
        "procMacro": {
          "enable": true,
          "ignored": {
            "async-trait": ["async_trait"],
            "napi-derive": ["napi"],
            "async-recursion": ["async_recursion"],
          },
        },
        "rust": {
          "analyzerTargetDir": true,
        },
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true,
          },
          "closureReturnTypeHints": {
            "enable": "always",
          },
        },
      },
    },
    "basedpyright": {
      "settings": {
        "python": {
          "pythonPath": ".venv/bin/python",
          "analysis": {
            "diagnosticSeverityOverrides": {
              "reportAny": "none",
              "reportMissingParameterType": "none",
              "reportMissingTypeArgument": "none",
              "reportMissingTypeStubs": "none",
              "reportUnknownArgumentType": "none",
              "reportUnknownLambdaType": "none",
              "reportUnknownMemberType": "none",
              "reportUnknownParameterType": "none",
              "reportUnknownVariableType": "none",
            },
          },
        },
      },
    },
    "ansible-language-server": {
      "settings": {
        "ansible": {
          "path": "ansible",
        },
        "executionEnvironment": {
          "enabled": false,
        },
        "python": {
          "interpreterPath": "python3",
        },
        "validation": {
          "enabled": true,
          "lint": {
            "enabled": true,
            "path": "ansible-lint",
          },
        },
      },
    },
    "gopls": {
      "initialization_options": {
        "gofumpt": true,
        "usePlaceholders": false,
        "completeUnimported": true,
        "staticcheck": false,
        "semanticTokens": true,
        "directoryFilters": ["-.git", "-.vscode", "-.idea", "-.vscode-test", "-node_modules"],
        "experimentalPostfixCompletions": true,
        "codelenses": {
          "gc_details": true,
          "generate": true,
          "regenerate_cgo": true,
          "run_govulncheck": true,
          "test": true,
          "tidy": true,
          "upgrade_dependency": true,
          "vendor": true,
        },
        "hints": {
          "assignVariableTypes": true,
          "compositeLiteralFields": true,
          "compositeLiteralTypes": true,
          "constantValues": true,
          "functionTypeParameters": true,
          "parameterNames": true,
          "rangeVariableTypes": true,
        },
        "analyses": {
          "nilness": true,
          "unusedparams": true,
          "unusedwrite": true,
          "useany": true,
        },
      },
    },
    "golangci-lint": {
      "initialization_options": {
        "command": ["golangci-lint", "run", "--output.json.path", "stdout", "--show-stats=false", "--output.text.path="],
      },
    },
  },
  "languages": {
    "Python": {
      "format_on_save": "on",
      "formatter": [
        {
          "external": {
            "command": "bash",
            "arguments": ["-c", "ruff check --select=I --fix --stdin-filename {buffer_path} | ruff format --stdin-filename {buffer_path}"],
          },
        },
      ],
      "language_servers": ["ty", "..."],
    },
    "JavaScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true,
      },
    },
    "Go": {
      "code_actions_on_format": {
        "source.organizeImports": true,
      },
      "format_on_save": "on",
      // // NOTE: add wsl_v5 as an extra formatter
      // "formatter": {
      //   "external": {
      //     "command": "golangci-lint",
      //     "arguments": ["run", "--no-config", "--enable-only", "wsl_v5", "--fix"]
      //   }
      // },
      "language_servers": ["!harper-ls", "..."],
    },
    "Markdown": {
      "format_on_save": "on",
      // "language_servers": ["!harper-ls", "..."]
    },
    "Terraform": {
      "format_on_save": "on",
      // NOTE: official terraform extension formatter doesn't work
      "formatter": {
        "external": {
          "command": "tofu",
          "arguments": ["fmt", "-"],
        },
      },
    },
    "Fish": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "fish_indent",
          "arguments": [],
        },
      },
    },
  },
  "auto_install_extensions": {
    "ansible": true,
    "basher": true,
    "csv": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "fish": true,
    "git-firefly": true,
    "github-actions": true,
    "golangci-lint": true,
    "harper": true,
    "helm": true,
    "html": true,
    "latex": true,
    "log": true,
    "lua": true,
    "make": true,
    "markdown-oxide": true,
    "material-icon-theme": true,
    "nginx": true,
    "nix": true,
    "opencode": true,
    "prisma": true,
    "scheme": true,
    "sql": true,
    "terraform": true,
    "tokyo-night": true,
    "toml": true,
    "vscode-icons": true,
    "wakatime": true,
    "zig": true,
  },
}
