#!/bin/bash

# FONTTEST SCRIPT
# -----------------
# Print all avaliable variants of chosen terminal font

echo -en "Normal\n"
echo -en "\033[1mBold\033[22m\n"
echo -en "\033[3mItalic\033[23m\n"
echo -en "\033[3;1mBold Italic\033[0m\n"
echo -en "\033[4mUnderline\033[24m\n"
echo -e "\e[9mStrikethrough\e[0m"

echo "== === !== >= <= =>"
echo "契          勒 鈴 "
